{
  "name": "linguabook-components",
  "version": "1.0.0",
  "license": "Apache",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:js build:css",
    "build:js": "tsc",
    "build:css": "cpx \"src/**/*.{scss,gif,png,jpg}\" dist",
    "dev": "run-p dev:js dev:css",
    "dev:js": "tsc --watch",
    "dev:css": "yarn build:css --watch",
    "start": "yarn dev",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.9.3",
    "axios": "^0.21.1",
    "clsx": "^1.2.0",
    "country-code-lookup": "^0.0.20",
    "dom-helpers": "^5.2.1",
    "eventemitter3": "^4.0.7",
    "exenv": "^1.2.2",
    "framer-motion": "^3.10.6",
    "lingua-scraper": "0.17.4",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react-content-loader": "^6.2.0",
    "react-ga": "^3.3.1",
    "react-icons": "^4.4.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-ga": "^1.2.3",
    "readline": "^1.3.0",
    "recoil": "^0.7.4",
    "swiper": "^6.7.5",
    "swr": "^1.3.0",
    "use-debounce": "^8.0.1",
    "use-force-update": "^1.0.8"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.4",
    "@types/exenv": "^1.2.0",
    "@types/react-responsive": "^8.0.3",
    "@types/react-router-dom": "^5.1.8"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
