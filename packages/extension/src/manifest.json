{
  "name": "LinguaBook",
  "author": "Sergey Todyshev",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "Browser Extension for https://linguabook.github.io",
  "icons": {
    "16": "assets/logo16.png",
    "48": "assets/logo48.png",
    "128": "assets/logo128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "css": ["lb.css"],
      "js": ["lib/jquery.js", "lib/popper.js", "lib/interact.js", "lb.js"]
    }
  ],
  "host_permissions": [
    "https://tsvbits.com/*",
    "https://*.regie.com/*",
    "https://*.google.com/*",
    "https://*.microsoft.com/*",
    "https://github.com/*",
    "https://*.github.io/*",
    "https://linkedin.com/*",
    "https://*.slack.com/*",
    "https://*.twitter.com/*"
  ]
}
